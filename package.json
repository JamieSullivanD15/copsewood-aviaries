{
  "name": "copsewood-aviaries",
  "version": "1.0.0",
  "description": "Express web app for Copsewood",
  "main": "server.js",
  "engines": {
    "node": "8.11.4"
  },
  "scripts": {
    "-------- GENERAL --------": "",
    "start": "node ./dist/server",
    "dev": "npm-run-all -p watch-js dev-server",
    "dev-server": "nodemon ./src/server",
    "-------- BUILD --------": "",
    "clean": "rimraf dist",
    "clean-node": "rimraf node_modules",
    "build-server": "webpack  --config webpack.server-prod.js",
    "compile-server": "webpack  --config webpack.server-dev.js",
    "compile-client": "webpack  --config webpack.client-dev.js",
    "build-client": "webpack  --config webpack.client-prod.js",
    "-------- COMPILING --------": "",
    "babel-js": "babel src/public/js/client/*.js --out-file src/public/js/babel.js",
    "browserify-js": "browserify src/public/js/babel.js -o src/public/js/index.js",
    "compile-js": "webpack  --config webpack.client-dev.js",
    "compile-sass": "node-sass src/public/styles/scss/index.scss --output-style compressed > src/public/styles/index.css",
    "------- WATCHING --------": "",
    "watch-css": "onchange \"src/public/styles/scss/**/*.scss\" -- npm run compile-sass",
    "watch-js": "onchange \"src/public/js/client/*.js\" -- npm run compile-js",
    "watch": "npm-run-all -p watch-js watch-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamieSullivanD15/copsewood-aviaries.git"
  },
  "author": "Jamie Sullivan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamieSullivanD15/copsewood-aviaries/issues"
  },
  "homepage": "https://github.com/JamieSullivanD15/copsewood-aviaries#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-messages": "^1.0.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.3.2",
    "multer": "^1.4.1",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
